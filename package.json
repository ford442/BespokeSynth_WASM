{
  "name": "bespoke-synth-wasm",
  "version": "1.0.0",
  "description": "BespokeSynth WASM - A modular synth web app built with TypeScript and AssemblyScript",
  "main": "dist/index.js",
  "scripts": {
    "asbuild:debug": "asc assembly/index.ts --target debug --outFile build/debug.wasm --textFile build/debug.wat --sourceMap --debug",
    "asbuild:release": "asc assembly/index.ts --target release --outFile build/release.wasm --textFile build/release.wat --sourceMap --optimize",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "build:ts": "tsc",
    "build:webpack": "webpack --mode production",
    "build": "npm run asbuild && npm run build:ts && npm run build:webpack",
    "dev": "webpack serve --mode development --open",
    "clean": "rm -rf build dist node_modules",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "synth",
    "music",
    "wasm",
    "assemblyscript",
    "typescript",
    "audio"
  ],
  "author": "BespokeSynth Contributors",
  "license": "GPL-3.0",
  "devDependencies": {
    "@assemblyscript/loader": "^0.27.29",
    "assemblyscript": "^0.27.29",
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {}
}
